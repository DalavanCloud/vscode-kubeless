{
    "name": "vscode-kubeless",
    "displayName": "vscode-kubeless",
    "description": "Kubeless plugin for VSCode",
    "version": "0.0.1",
    "publisher": "test",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.deployFunction",
        "onCommand:extension.callFunction",
        "onCommand:extension.getFunctionLogs",
        "onCommand:extension.updateFunction",
        "onCommand:extension.deleteFunction"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.deployFunction",
            "title": "Deploy Function"
        },
        {
            "command": "extension.callFunction",
            "title": "Call Function"
        },
        {
            "command": "extension.getFunctionLogs",
            "title": "Get Function Logs"
        },
        {
            "command": "extension.updateFunction",
            "title": "Update Function"
        },
        {
            "command": "extension.deleteFunction",
            "title": "Delete Function"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^8.0.26",
        "@types/mocha": "^2.2.42"
    }
}
